<odoo>
    <template id="book_list_template" name="Book List">
        <div id="wrap" class="container">
            <h1>Books</h1>
            <t t-foreach="book" t-as="libro">
                <div class="row">
                    <span t-field="libro.name"/>
                    <span t-field="libro.date_published"/>
                    <span t-field="libro.publisher_id"/>
                </div>
            </t>
        </div>
    </template>

</odoo>